class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {        //vector为容器，可以动态的分配数组内存
        int i,j;
        for(i=0;i<nums.size()-1;i++)
        {
            for(j=i+1;j<nums.size();j++)
            {
                if(nums[i]+nums[j]==target)
                {
                   return {i,j};
                }
            }
        }
        return {i,j};
    };
};



